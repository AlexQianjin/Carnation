{
  "swagger": "2.0",
  "info": {
    "title": "Weapon API",
    "description": "Move your app forward with the Weapon API",
    "version": "1.0.0"
  },
  "host": "localhost:5000",
  "schemes": [
    "http"
  ],
  "basePath": "/api/v1",
  "tags": [
    {
      "name": "User",
      "description": "Everything about the User"
    },
    {
      "name": "Project",
      "description": "Everything about the Project"
    }
  ],
  "produces": [
    "application/json"
  ],
  "security": [
    {
      "weapon": []
    }
  ],
  "paths": {
    "/users": {
      "get": {
        "summary": "Users",
        "description": "The Users endpoint returns information about the Users.\n",
        "tags": [
          "User"
        ],
        "responses": {
          "200": {
            "description": "An array of users",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/User"
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    },
    "/project": {
      "get": {
        "summary": "Project",
        "description": "The Project endpoint returns information about the Project.\n",
        "tags": [
          "Project"
        ],
        "responses": {
          "200": {
            "description": "An object of project",
            "schema": {
              "type": "object",
              "items": {
                "$ref": "#/definitions/Project"
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "weapon": {
      "type": "oauth2",
      "authorizationUrl": "http://localhost:5000/v1/token",
      "flow": "implicit"
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier."
        },
        "username": {
          "type": "string",
          "description": "Username of user."
        },
        "password": {
          "type": "string",
          "description": "Password of user."
        }
      }
    },
    "Project": {
      "type": "object",
      "properties": {
        "projectName": {
          "type": "string",
          "description": "Unique identifier."
        },
        "nodes": {
          "type": "string",
          "description": "Username of user.",
          "items": {
            "$ref": "#/definitions/Node"
          }
        }
      }
    },
    "Node": {
      "type": "object",
      "properties": {
        "nodeName": {
          "type": "string",
          "description": "Node Name."
        },
        "parameters": {
          "type": "array",
          "description": "Parameters list.",
          "items": {
            "$ref": "#/definitions/Parameter"
          }
        }
      }
    },
    "Parameter": {
      "type": "object",
      "properties": {
        "keyName": {
          "type": "string",
          "description": "Key Name."
        },
        "value": {
          "type": "string",
          "description": "Key value."
        }
      }
    },
    "Error": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "fields": {
          "type": "string"
        }
      }
    }
  }
}